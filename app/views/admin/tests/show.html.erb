<% content_for :content_header do %>
    <section class="content-header">
      <h1>Show
        <small>admin_test</small>
      </h1>

      <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><%= link_to "Admin_tests", admin_tests_path %></li>
        <li class="active">Show</li>
      </ol>
    </section>
<% end %>

<section class="content">
  <div class="row">
    <div class="col-md-12">
      <div class="box box-primary">
        <div class="box-header">
          <div class="box-tools pull-right">
            <%= link_to edit_admin_test_path(@admin_test), :class => 'btn btn-primary' do %>
                <span class="glyphicon glyphicon-pencil"></span>
                Edit
            <% end %>
          </div>
        </div>
        <div class="box-body">
          <dl class="dl-horizontal">
            <dt>Test:</dt>
            <dd><%= @admin_test.name %></dd>
            <dt>Description:</dt>
            <dd><%= @admin_test.description %></dd>

          </dl>
        </div>
      </div>
    </div>
  </div>
</section>

